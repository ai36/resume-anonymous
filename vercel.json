{
    "version": 2,
    "builds": [
      {
        "src": "src/api/data.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/data",
        "dest": "/src/api/data.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }